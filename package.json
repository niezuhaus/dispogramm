    "@types/mapbox-gl": "^2.7.19",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.13",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
