{
  "name": "dispogramm",
  "version": "1.3.9",
  "scripts": {
    "ng": "ng",
    "start": "npx ng serve",
    "build": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch && npx ng build",
    "watch": "npx ng build --watch --configuration development",
    "test": "npx ng test",
    "build-serve": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --port 4200",
    "build-prod": "NODE_ENV=production npx ng build --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.1.0",
